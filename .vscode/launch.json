    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "simple debugging solution",
            "program": "${workspaceFolder}/out/build/x64-Darwin-Debug/bin/solution/solution",
            "args": [
                "--fib"
            ],
            "cwd": "${workspaceFolder}",
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "debugging test_solution findKthLargest.example1",
            "program": "${workspaceFolder}/out/build/x64-Darwin-Debug/tests/solution/tests_solution",
            "args": [
                "--gtest_filter=findKthLargest.example1",
                "--gtest_also_run_disabled_tests"
            ],
            "cwd": "${workspaceFolder}",
        }
    ]
}
